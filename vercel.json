{
  "buildCommand": "yarn build",
  "devCommand": "yarn start:dev",
  "installCommand": "yarn",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "dist/src/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
